#about
	// height 1359px
	padding 175px 0 100px
	margin-top: -75px
	background-image: url(../img/backs/about.svg)
	background-repeat no-repeat
	background-position: center top
	background-size: cover
	position relative
	z-index 90
	@media screen and (max-width 991px)
		padding 130px 0 100px
		margin-top: -45px
	@media screen and (max-width 767px)
		padding 90px 0 60px
.about-img
	position absolute
	top -100px
	left -150px
	width 539px
	height: 999px
	background-image: url(../img/images/about-d.png)
	background-repeat no-repeat
	background-position: center
	background-size contain
	@media screen and (max-width 1199px)
		left -200px
	@media screen and (max-width 991px)
		display none
.circle-about
	width 300px
	height: 300px
	border-radius: 50%
	background-image: url(../img/images/circle-1.png)
	background-repeat no-repeat
	background-position: center
	background-size contain
	position absolute
	top -50px
	right -270px
	@media screen and (max-width 991px)
		top auto
		bottom: 0
	@media screen and (max-width 560px)
		width 200px
		height: 200px
		bottom: -100px
		right -100px
	@media screen and (max-width 500px)
		bottom: 0
.circle-about-img-1
	width 265px
	height: 265px
	bottom: 50px
	left -190px
	@media screen and (max-width 991px)
		display none
.circle-about-img-2
	width 421px
	height: 421px
	top 100px
	right -355px
	@media screen and (max-width 991px)
		display none
.about
	&-content
		margin-left: 380px
		@media screen and (max-width 1199px)
			margin-left: 320px
		@media screen and (max-width 991px)
			margin-left: 0
		@media screen and (max-width 500px)
			.col-xs-6
				width 100%
	&-quote
		&-title
			position relative
			color #416BD8
			font-family: G
			font-size: 38px
			font-weight: bold
			text-transform uppercase
			margin 34px 0 30px 66px
			@media screen and (max-width 1199px)
				font-size: 32px
			@media screen and (max-width 991px)
				font-size: 30px
			@media screen and (max-width 767px)
				font-size: 26px
			@media screen and (max-width 560px)
				margin 20px 0 20px 30px
			@media screen and (max-width 386px)
				font-size: 20px
				margin 20px 0 20px 15px
			&:before
				content: ''
				display block
				position absolute
				top -28px
				left -66px
				width 88px
				height: 56px
				background-image: url(../img/icons/about-quotes.png)
				background-repeat: no-repeat
				background-position: center
				background-size contain
				@media screen and (max-width 991px)
					width 66px
					height: 46px
					top -18px
					left -46px
				@media screen and (max-width 560px)
					width 48px
					height: 48px
					left -26px
				@media screen and (max-width 386px)
					left -30px
			span
				position relative
				&:before
					content: ''
					display block
					position absolute
					top 0
					right -66px
					width 88px
					height: 56px
					background-image: url(../img/icons/about-quotes.png)
					background-repeat: no-repeat
					background-position: center
					background-size contain
					@media screen and (max-width 991px)
						width 66px
						height: 46px
						top 10px
						right -48px
					@media screen and (max-width 560px)
						width 48px
						height: 48px
						right -36px
						top 5px
		&-text
			span
				font-weight: bold
			&:last-of-type
				margin-left: 230px
				@media screen and (max-width 1199px)
					margin-left: 150px
				@media screen and (max-width 560px)
					margin-left: 50px
				@media screen and (max-width 386px)
					margin-left: 25px
	&-content-title
		&s
			margin 60px auto 76px
			position relative
			@media screen and (max-width 991px)
				margin 35px auto 40px
			@media screen and (max-width 560px)
				margin 35px 0 20px
		font-size: 80px
		font-weight: 900
		text-transform uppercase
		color #EAEAEA
		text-shadow: 2px 2px 0px #fff, 2px 0 0px #fff, 0 2px 0px #fff,
		-2px 2px 0px #fff, 2px -2px 0px #fff, -2px 0 0px #fff, 0 -2px 0px #fff
		line-height:50px
		@media screen and (max-width 1199px)
			font-size: 60px
			line-height: 30px
		@media screen and (max-width 560px)
			font-size: 40px
			line-height: 10px
			margin-bottom: -6px
		&-img
			position relative
			left 20px
			top -5px
			display block
			width 100%
			max-width 786px
			height: 75px
			background-image: url(../img/images/about-text.png)
			background-repeat no-repeat
			background-position: center
			background-size contain
			@media screen and (max-width 991px)
				max-width 686px
			@media screen and (max-width 767px)
				background-image: url(../img/images/about-text-m.png)
				max-width 484px
			@media screen and (max-width 560px)
				max-width 380px
			@media screen and (max-width 386px)
				left 5px
				top -10px
	&-item
		&-title
			font-size: 45px
			font-weight: 900
			color #0D3397
			text-transform uppercase
			line-height: 34px
			@media screen and (max-width 1199px)
				font-size: 32px
				line-height: 30px
			@media screen and (max-width 560px)
				font-size: 26px
				line-height: 24px
			@media screen and (max-width 500px)
				margin-bottom: 25px
			small
				display block
				font-size: 22px
				font-weight: bold
				text-transform none
				margin-top: 20px
				@media screen and (max-width 1199px)
					font-size: 18px
				@media screen and (max-width 991px)
					font-size: 16px
					margin-top: 10px
				@media screen and (max-width 560px)
					margin-top: 5px
.button-about
	margin 145px auto 0
	@media screen and (max-width 991px)
