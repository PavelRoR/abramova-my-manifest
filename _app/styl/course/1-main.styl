#main
	position relative
	margin-bottom: 80px
	color #fff
	@media screen and (max-width 991px)
		background-color: #121112
		background-image: url(../img/backs/main-d.png)
		background-repeat no-repeat
		background-position: top left -200px
		background-size: cover
		padding 60px 0
		margin 0
		&:before
			content: ''
			display block
			position absolute
			top 0
			left 0
			width 100%
			height: 100%
			background-color: rgba(#000, .7)
	@media screen and (max-width 560px)
		background-image: url(../img/backs/main-m.png)
		background-position: top left
		&:before
			background-color: rgba(#000, .55)
	@media screen and (max-width 386px)
		padding 45px 0
.wrapper-abs
	position absolute
	top 0
	left 0
	right 0
	bottom: 0
	@media screen and (max-width 991px)
		position: relative
	&-main
