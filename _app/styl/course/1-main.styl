			padding 120px 0 0
			height 100%
			@media screen and (max-width 1199px)
				padding-top: 70px
			@media screen and (max-width 991px)
				padding 0
.main-wrapper
	position absolute
	top: 0
	left 0
	display table
	width 100%
	height 100%
	@media screen and (max-width 991px)
		position: static
		display block
.main-content
	display table-cell
	vertical-align: middle
	width 100%
	position relative
	@media screen and (max-width 991px)
		display block
.main-bg
	display block
	width 1440px
	object-fit: cover
	object-position: center
	height 100%
	max-height: 940px
