		&&-mail-main
			border-color #fff
			color rgba(#fff,.55)
			&::-webkit-input-placeholder
				color:rgba(#fff,.55)
				opacity:1;
			&::-moz-placeholder
				color:rgba(#fff,.55)
				opacity:1;
			&:-ms-input-placeholder
				color:rgba(#fff,.55)
				opacity:1;
			&:active
			&:focus
			&:active
				border-color: #fff
				color #fff
	&::-webkit-input-placeholder
		color:#D9D9D9
		opacity:1;
	&::-moz-placeholder
		color:#D9D9D9
		opacity:1;
	&:-ms-input-placeholder
		color:#D9D9D9
		opacity:1;
@-webkit-keyframes autofill
	to
		font-size: 22px
		color: #D9D9D9;
		background-color: transparent
input:-webkit-autofill
	-webkit-animation-name: autofill;
	-webkit-animation-fill-mode: both;
.input:active
input:-internal-autofill-selected
	background-color: transparent !important
.alert-message
	clear both
	display: none;
	font-size: 16px;
	padding-top: 10px;
	color: #f00
	text-align: center
	font-weight: 700
	line-height: 1
.checking
	display: table;
	width 100%
	// max-width 455px
	margin: 5px auto 0
	text-align: left
@keyframes pulse
	0%
		box-shadow: 0 0 0 0 rgba(red, .12)
	100%
		box-shadow: 0 0 15px 10px rgba(red, .12)
.checking .check-label
	line-height: 22px;
	cursor: pointer;
	position: relative;
	display: table-cell;
	vertical-align: middle;
	padding: 0 0 0 30px;
	text-align: left;
	font-size: 14px
	color #4E4C4C
	@media screen and (max-width 1199px)
		font-size: 12px
		line-height: 18px
	@media screen and (max-width 560px)
		font-size: 10px
.checking .check-label a
	text-decoration: none
	border-bottom: 1px dashed
	display: inline
	color #4e4e4e
	&:focus
		border-bottom-color:
		color #4e4e4e
	&:hover
		border-bottom-color: transparent
		text-decoration: none
		color #4e4e4e
.checking-main .check-label
	color #aaa
.checking-main .check-label a
	color #aaa
	&:focus
	&:hover
		color #aaa
.checking .check-label:before
	content: '';
	background-color: red;
	position: absolute;
	left: 0;
	top: 3px;
	width: 25px;
	height: 25px;
	border none
	border-radius: 5px
	animation: pulse
	animation-duration: .7s
	animation-iteration-count: infinite
	animation-timing-function: ease-out;
.checking .check-label:after
	content: '';
	position: absolute;
	left: 9px;
	top: -10px;
	width: 8px;
	height: 14px;
	background-color: transparent;
	opacity: 0;
	border-bottom: 2px solid #fff;
	border-right: 2px solid #fff;
	-webkit-transform: rotate(41deg);
	-ms-transform: rotate(41deg);
	transform: rotate(41deg);
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;
.check
	position: absolute;
	z-index: -1;
	opacity: 0;
	width: 0;
	height 0;
	border: 0;
	left -99999px;
.check:checked + .check-label:before
	background-color: #416BD8
	animation: none
.check:checked + .check-label:after
	opacity: 1;
	top: 6px;
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;